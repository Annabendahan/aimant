{
  "name": "aimant-website",
  "private": true,
  "description": "Official Aimant Website",
  "version": "0.1.0",
  "author": "Thomas & Anna & Kevin",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@u-wave/react-vimeo": "^0.9.2",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^2.29.2",
    "gatsby-image": "^2.8.0",
    "gatsby-plugin-manifest": "^2.9.1",
    "gatsby-plugin-offline": "^3.7.1",
    "gatsby-plugin-react-helmet": "^3.7.0",
    "gatsby-plugin-sass": "^2.8.0",
    "gatsby-plugin-sharp": "^2.11.2",
    "gatsby-plugin-styled-components": "^3.7.0",
    "gatsby-source-filesystem": "^2.8.1",
    "gatsby-source-graphql": "^2.11.0",
    "gatsby-source-wordpress": "^3.8.1",
    "gatsby-transformer-sharp": "^2.9.0",
    "gatsby-wordpress-inline-images": "^1.2.1",
    "gatsby-wpgraphql-inline-images": "^0.2.5",
    "html-react-parser": "^1.1.1",
    "node-sass": "4.14.1",
    "npm-check-updates": "^10.2.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "simple-react-lightbox": "^3.3.4",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "prettier": "2.2.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "format:css": "prettier --write \"src/**/*.scss\"",
    "restore": "rm -rf node_modules/ && yarn install",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Annabendahan/aimant/"
  },
  "bugs": {
    "url": "https://github.com/Annabendahan/aimant/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write"
    ],
    "src/**/*.scss": [
      "prettier --write"
    ]
  }
}
